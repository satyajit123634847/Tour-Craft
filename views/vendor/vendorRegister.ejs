    // Function to save data and show the next section
    function saveAndContinue(nextSection) {
      // alert(nextSection)
      // Save the data here (you can retrieve the values using document.getElementById())

      // Hide the current section
      var currentSection = document.querySelector(".active-section");
      currentSection.style.display = "none";
      currentSection.classList.remove("active-section");

      // Show the next section
      var nextSectionElement = document.getElementById(nextSection);
      nextSectionElement.style.display = "block";
      nextSectionElement.classList.add("active-section");
    }

    // Function to go back to the previous section
    function goBack(previousSection) {
      // Hide the current section
      var currentSection = document.querySelector(".active-section");
      currentSection.style.display = "none";
      currentSection.classList.remove("active-section");

      // Show the previous section
      var previousSectionElement = document.getElementById(previousSection);
      previousSectionElement.style.display = "block";
      previousSectionElement.classList.add("active-section");
    }

